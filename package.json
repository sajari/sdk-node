{
  "name": "@rentpath/sajari-sdk-node",
  "version": "3.3.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Ben Hinchley <bhinchley@sajari.com>",
  "license": "MIT",
  "files": [
    "README.md",
    "LICENSE",
    "dist/",
    "generated/"
  ],
  "engines": {
    "node": ">= 10.0.0"
  },
  "scripts": {
    "prebuild": "npm-run-all gen:useragent lint test",
    "build": "tsc",
    "fmt": "prettier --write --config .prettierrc.js \"{src/**/*,*.config,*.d,.*,package}.{js,ts,json}\"",
    "lint": "tslint --fix -p tsconfig.json",
    "type-check": "tsc --noEmit",
    "pretest": "npm-run-all type-check",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "gen:proto": "node scripts/genproto",
    "gen:useragent": "node scripts/genuseragent",
    "docs": "typedoc ./src/index.ts --mode file --out ./docs --exclude \"**/*+(.test).ts\"",
    "prepublishOnly": "npm-run-all build"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.2.5",
    "debug": "^4.1.1",
    "deepmerge": "^4.0.0",
    "protobufjs": "^6.8.8"
  },
  "devDependencies": {
    "@types/debug": "^4.1.4",
    "@types/jest": "^24.0.15",
    "globby": "^10.0.1",
    "grpc-tools": "^1.8.0",
    "jest": "^24.8.0",
    "make-dir": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  }
}
