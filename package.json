{
  "name": "@sajari/sdk-node",
  "version": "2.0.0-beta.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Ben Hinchley <bhinchley@sajari.com>",
  "license": "MIT",
  "files": [
    "README.md",
    "LICENSE",
    "dist/",
    "generated/"
  ],
  "engines": {
    "node": ">= 10.0.0"
  },
  "scripts": {
    "prebuild": "npm-run-all lint test",
    "build": "tsc",
    "fmt": "prettier --write --config .prettierrc.js \"{src/**/*,*.config,*.d,.*,package}.{js,ts,json}\"",
    "lint": "tslint --fix -p tsconfig.json",
    "type-check": "tsc --noEmit",
    "pretest": "npm-run-all type-check",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "gen:proto": "node scripts/genproto",
    "docs": "typedoc ./src/index.ts --mode file --out ./docs --exclude \"**/*+(.test).ts\"",
    "prepublishOnly": "npm-run-all build"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "deepmerge": "^3.2.0",
    "grpc": "1.19.0",
    "protobufjs": "^6.8.8"
  },
  "devDependencies": {
    "@types/debug": "^4.1.2",
    "@types/jest": "^24.0.11",
    "globby": "^9.1.0",
    "grpc-tools": "^1.7.1",
    "jest": "^24.5.0",
    "make-dir": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3333"
  }
}
